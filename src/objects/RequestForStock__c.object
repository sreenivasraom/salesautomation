<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <validationRules>
        <fullName>stocklimit</fullName>
        <active>true</active>
        <description>stocklimit - Demo</description>
        <errorConditionFormula>Current_Stock__c + RequiredStock__c &lt; 500</errorConditionFormula>
        <errorMessage>stock limit exceeds 500</errorMessage>
    </validationRules>
</CustomObject>
